{
  "name": "fem-typescript-library",
  "packageManager": "yarn@3.8.7",
  "main": "dist/index.js",
  "types": "dist/fem-typescript-library.d.ts",
  "scripts": {
    "build": "yarn tsc",
    "dev": "yarn build --watch --preserveWatchOutput",
    "lint": "yarn eslint src --ext js,ts",
    "watch": "concurrently -c 'red,blue' -n 'TS,JEST' \"yarn watch-build\" \"yarn watch-tests\"",
    "watch-build": "yarn tsc --watch --preserveWatchOutput",
    "watch-tests": "yarn jest --watch",
    "test": "yarn build && yarn jest"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@microsoft/api-documenter": "^7.27.1",
    "@microsoft/api-extractor": "^7.53.1",
    "@types/eslint": "^9",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "jest": "^30.2.0",
    "typescript": "~5.3.0"
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "3.6.4"
  }
}
